.navHeader{
    margin-bottom: 20rpx;
}
.navHeader .title{
    font-size: 26rpx;
    color: #666;
}
.navHeader .more{
    display: flex;
    justify-content: space-between;
}
.navHeader.more text:nth-of-type(2){
    padding: 10rpx 20rpx;
    border: 1px solid #333;
    font-size: 26rpx; 
    border-radius: 30rpx;
    font-weight: bold;
}